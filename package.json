{
  "name": "mimik",
  "version": "0.4.5",
  "description": "UI Automation made easy!",
  "main": "./bin/mimik",
  "scripts": {
    "web": "NODE_ENV=development node app/index.js",
    "test": "mocha --recursive --reporter spec tests/*",
    "coverage": "istanbul cover _mocha --recursive tests/*",
    "lint": "jshint ."
  },
  "bin": {
    "mimik": "./bin/mimik"
  },
  "engines": {
    "node": ">=0.10",
    "npm": ">=1.3"
  },
  "keywords": [
    "ui",
    "automation",
    "automated",
    "test",
    "tests",
    "functional",
    "integration",
    "testing",
    "browser",
    "BDD",
    "spec",
    "DSL",
    "gherkin",
    "GWT",
    "given",
    "when",
    "then",
    "feature",
    "scenario",
    "grammar",
    "html",
    "dom",
    "javascript",
    "gui",
    "coffee",
    "coffeescript",
    "user",
    "interactive",
    "interaction",
    "click",
    "mouse",
    "keyboard"
  ],
  "author": "Simo Moujami <simo@webinventinc.com>",
  "contributors": [
    "Simo Moujami <simo@webinventinc.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/simoami/mimik.git"
  },
  "dependencies": {
    "colors": "~0.6.2",
    "commander": "~2.2.0",
    "glob": "~3.2.9",
    "yadda": "~0.10.8",
    "coffee-script": "~1.7.1",
    "async": "~0.8.0",
    "cli-table": "~0.3.0",
    "mocha": "~1.18.2",
    "winston": "~0.7.3",
    "didyoumean": "~1.2.0",
    "webdriverjs": "~1.6.1",
    "dot": "~1.0.2",
    "chai": "~1.9.1",
    "fs-tools": "~0.2.11",
    "moment": "~2.6.0",
    "request": "~2.34.0",
    "chokidar": "~0.8.2"
  },
  "devDependencies": {
    "jshint": "~2.5.0",
    "express": "~4.1.1"
  },
  "testConfig": {
    "port": 3000
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/simoami/mimik/blob/master/LICENSE"
  }
}
